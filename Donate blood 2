#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int age;
    float weight, hemoglobin;

    
    scanf("%d", &age);
    scanf("%f", &weight);
    scanf("%f", &hemoglobin);

    
    if (age >= 18 && weight >= 45 && hemoglobin >= 5.5) {
        printf("He or She can donate blood\n");
    } else {
        printf("He or She unable to donate blood\n");
    }

    return 0;
}

 
