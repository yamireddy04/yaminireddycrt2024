import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String str = scanner.nextLine();
        for (int i = 0; i < str.length(); i++) {
            if (i % 2 == 0) {
                System.out.println(i + " " + str.charAt(i));
            }
        }
    }
}
